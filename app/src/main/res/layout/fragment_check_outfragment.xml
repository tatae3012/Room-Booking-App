<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:calendar="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aryan.android.oyo.CheckOutfragment">

    <!-- TODO: Update blank fragment layout -->
    <com.andexert.calendarlistview.library.DayPickerView
        android:id="@+id/pickerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:colorSelectedDayBackground="#ffff0000"
        app:colorSelectedDayText="#fff"
        app:colorPreviousDay="#d3d3d3"
        app:colorNormalDay="#000"
        app:colorMonthName="#000"
        app:colorDayName="#808080"
        app:textSizeDay="12sp"
        app:calendarHeight="300dip"
        app:enablePreviousDay="false"
        calendar:singleSelectionOnly="true"
        />

</FrameLayout>
